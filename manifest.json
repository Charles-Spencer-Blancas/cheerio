{
    "name": "Cheerio",
    "description": "A chrome extension that reminds you to take care of yourself when browsing the web.",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "assets/128.png"
        }
    },
    "permissions": ["notifications","alarms","storage"]
}